		"typescript": "^5.8.3",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@sanity/block-content-to-html": "^2.0.0",
		"@sanity/client": "^6.29.0",
		"@sanity/image-url": "^1.1.0",
		"lodash-es": "^4.17.21"
	}
