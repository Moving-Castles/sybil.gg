		"sass-embedded": "^1.93.2",
		"svelte": "^5.42.3",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.1.12"
	},
	"dependencies": {
		"@sanity/block-content-to-html": "^2.0.0",
		"@sanity/client": "^7.12.0",
		"@sanity/image-url": "^1.2.0",
		"lodash-es": "^4.17.21"
	}
